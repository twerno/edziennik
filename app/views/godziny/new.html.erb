<h1>New godzina</h1>

<% form_for(@godzina) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :nazwa %><br />
    <%= f.text_field :nazwa %>
  </p>
  <p>
    <%= f.label :begin %><br />
    <%= f.time_select :begin %>
  </p>
  <p>
    <%= f.label :end %><br />
    <%= f.time_select :end %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', godziny_path %>
