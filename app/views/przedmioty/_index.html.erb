<h1>Listing przedmioty</h1>

<table>
  <tr>
    <th>Nazwa</th>
  </tr>

<% for przedmiot in @przedmioty %>
  <tr>
    <td><%=h przedmiot.nazwa %></td>
    <td><%= link_to 'Show', przedmiot %></td>
    <td><%= link_to 'Edit', edit_przedmiot_path(przedmiot) %></td>
    <td><%= link_to 'Destroy', przedmiot, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New przedmiot', new_przedmiot_path %>
